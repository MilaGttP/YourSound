
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <FontFamily x:Key="ExtraBold">/Fonts/Montserrat-ExtraBold.ttf#Montserrat</FontFamily>
    <FontFamily x:Key="SemiBold">/Fonts/Montserrat-SemiBold.ttf#Montserrat</FontFamily>
    <FontFamily x:Key="Light">/Fonts/Montserrat-Light.ttf#Montserrat</FontFamily>

    <Style x:Key="MainWindowStyle" TargetType="Window">
        <Setter Property="Background" Value="#3B2E59" />
    </Style>

</ResourceDictionary>